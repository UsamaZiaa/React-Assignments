this.setState({\r\n          todos: this.state.todos\r\n        })\r\n      }\r\n\r\n    render() {\r\n        let {todos, value} = this.state;\r\n        return (\r\n            <div>\r\n                <h1>Todo App</h1>\r\n              <input value={value} onChange={(e)=>this.setState({value: e.target.value})} type=\"text\" placeholder=\"Enter Todo\"/>\r\n              {/* <p>{this.state.value}My Para</p> */}\r\n              <button onClick={this.add_todo}>Add Item</button>\r\n              <ul>\r\n                {todos.map((v,i)=>{\r\n                  return <li key={i}>\r\n                    {v.edit ? <input value={v.title} type=\"text\" onChange={(e) => this.handleChange(e, i)}/> : v.title}\r\n                    {v.edit ?\r\n                      <button onClick={() => this.update(i)}>Update</button>\r\n                      :\r\n                      <button onClick={() => this.edit_todo(i)}>Edit</button>\r\n                    }  \r\n                  <button onClick={() => this.delete_todo(i)}>Delete</button>\r\n                  </li>\r\n                })}\r\n              </ul>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default TodoApp;",["46","47"],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\index.js",[],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\App.jsx",[],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\components\\counter.jsx",[],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\components\\TextToggle.jsx",[],["48","49"],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\components\\StopWatch.jsx",[],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\components\\GithubProfile.jsx",[],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\reportWebVitals.js",[],["50","51"],"C:\\Users\\Usama\\Desktop\\Web And Mobile App Course\\React\\todo-app\\src\\components\\Login.jsx",["52","53"],"import React from 'react'\r\n\r\nclass Login extends React.Component{\r\n    constructor(){\r\n        super()\r\n        this.state = {\r\n            email: '',\r\n            password: '',\r\n            enableBtn: true,\r\n            emailError: false,\r\n            passwordError: false\r\n        }\r\n    }\r\n\r\n    handleEmail = (event) =>{\r\n        // console.log(event.target.value)\r\n        this.setState({\r\n            email: event.target.value\r\n        })\r\n    } \r\n\r\n    handlePassword = (event) =>{\r\n        // console.log(event.target.value)\r\n        this.setState({\r\n            password: event.target.value\r\n        })\r\n    } \r\n\r\n    onLogin = () => {\r\n        const re = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n        console.log(\"EMAIL ==> \",this.state.email)\r\n        console.log(\"PASSWORD ==> \",this.state.password)\r\n\r\n        if(!re.test(String(this.state.email).toLowerCase())){\r\n            this.setState({\r\n                emailError: true\r\n            })\r\n            return\r\n        }else{\r\n            this.setState({\r\n                emailError: false\r\n            })\r\n        }\r\n\r\n        if(this.state.password.length<6){\r\n            this.setState({\r\n                passwordError: true\r\n            })\r\n            return\r\n        }else{\r\n            this.setState({\r\n                passwordError: false\r\n            })\r\n        }\r\n\r\n        this.setState({\r\n            email: '',\r\n            password: ''\r\n        })\r\n    }\r\n\r\n    render(){\r\n        return(\r\n            <div>\r\n                <h1>Login Form</h1>\r\n                <label>\r\n                    Email <br/>\r\n                    <input value={this.state.email} type=\"email\" placeholder=\"Enter your email\" onChange={this.handleEmail}/>\r\n                </label>\r\n                <label> <br/>\r\n                    Password <br/>\r\n                    <input value={this.state.password} type=\"password\" placeholder=\"Enter your password\" onChange={this.handlePassword}/>\r\n                </label> <br/>\r\n                <p style={{color: 'red'}}>\r\n                    {this.state.emailError ? \"Please insert a valid Email\": '' }\r\n                    {this.state.passwordError ? \"Password must be 6 Character Long\": '' }\r\n                </p>\r\n                <button disabled={this.state.email === '' || this.state.password === '' ? true : false} onClick={this.onLogin}>Login</button>\r\n            </div>\r\n        )\r\n    }   \r\n}\r\n\r\nexport default Login;",{"ruleId":"54","severity":1,"message":"55","line":30,"column":9,"nodeType":"56","endLine":30,"endColumn":32},{"ruleId":"54","severity":1,"message":"55","line":37,"column":9,"nodeType":"56","endLine":37,"endColumn":32},{"ruleId":"54","severity":1,"message":"55","line":44,"column":9,"nodeType":"56","endLine":44,"endColumn":32},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"57","replacedBy":"61"},{"ruleId":"59","replacedBy":"62"},{"ruleId":"57","replacedBy":"63"},{"ruleId":"59","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":30,"column":30,"nodeType":"67","messageId":"68","endLine":30,"endColumn":31,"suggestions":"69"},{"ruleId":"65","severity":1,"message":"66","line":30,"column":55,"nodeType":"67","messageId":"68","endLine":30,"endColumn":56,"suggestions":"70"},"react/no-direct-mutation-state","Do not mutate state directly. Use setState().","MemberExpression","no-native-reassign",["71"],"no-negated-in-lhs",["72"],["71"],["72"],["71"],["72"],"no-useless-escape","Unnecessary escape character: \\[.","Literal","unnecessaryEscape",["73","74"],["75","76"],"no-global-assign","no-unsafe-negation",{"messageId":"77","fix":"78","desc":"79"}